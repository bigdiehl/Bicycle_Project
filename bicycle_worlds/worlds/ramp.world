<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Set default camera pose -->
    <gui>
      <camera name="user_camera">
          <pose> -2.6 -6.2 7.5 0 0.65 0.91 </pose>
      </camera>
    </gui>

  </world>
</sdf>